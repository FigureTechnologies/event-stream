{
  "extends": [
    "config:base"
  ],
  "enabledManagers": ["regex"],
  "regexManagers": [
    {
      "fileMatch": ["(^|/)Dockerfile%"],
      "matchStrings": ["FROM openjdk:(?<currentValue>.*?)\n"],
      "depNameTemplate": "openjdk",
      "datasourceTemplate": "adoptium-java"
    }
  ],
  "schedule": [
    "before 8am on Monday"
  ]
}
